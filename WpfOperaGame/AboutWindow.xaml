<Window x:Class="WpfOperaGame.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfOperaGame"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="AboutWindow" Height="450" Width="800">
    <Grid>
        <Label Content="About" Style="{StaticResource ResourceKey=titleLabel}"/>
        <Button x:Name="backButton" Content="Back" VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="0,0,0,30" Click="backButton_Click"/>
    </Grid>
</Window>
