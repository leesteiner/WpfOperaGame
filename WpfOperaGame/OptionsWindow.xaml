<Window x:Class="WpfOperaGame.OptionsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Options" Height="450" Width="800">
    <Grid>
        <Label Style="{StaticResource ResourceKey=titleLabel}"  Content="Options"/>
        <Button x:Name="backButton" Content="Back" VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="0,0,0,30" Click="backButton_Click"/>
    </Grid>
</Window>
